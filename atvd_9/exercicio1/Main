        funcionarios.add( new Funcionario("Vini", 4000, 47));
        funcionarios.add( new Funcionario("Paul", 20000, 55));
        int i=funcionarios.size();
         for (Funcionario funcionario : funcionarios) {
            if(funcionario.getIdade()<=20 && funcionario.getSalario()>=30000){
            System.out.println("Nome: " + funcionario.getNome() + "\n" +"Sal√°rio anual: " + funcionario.getSalario() + "\n" +"Idade: " + funcionario.getIdade() + "\n");
            }
        }
    }
}
